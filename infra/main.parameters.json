{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "name": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "openAiResourceName": {
        "value": "${AZURE_OPENAI_RESOURCE}"
      },
      "openAiResourceGroupName": {
        "value": "${AZURE_OPENAI_RESOURCE_GROUP}"
      },
      "openAiResourceLocation": {
        "value": "${AZURE_OPENAI_RESOURCE_LOCATION}"
      },
      "openAiSkuName": {
        "value": "${AZURE_OPENAI_SKU_NAME}"
      },
      "createRoleForUser": {
        "value": "${CREATE_ROLE_FOR_USER=true}"
      },
      "acaExists": {
        "value": "${SERVICE_ACA_RESOURCE_EXISTS=false}"
      },
      "allowedOrigins": {
        "value": "${ALLOWED_ORIGINS}"
      },
      "runningOnGh": {
        "value": "${GITHUB_ACTIONS}"
      }
    }
  }
